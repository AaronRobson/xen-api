OCAML_LIBS    = ../util/version ../idl/xapi-consts ../database/xapi-database ../xapi-types/xapi-types ../xapi-client/xapi-client ../util/stats 
OCAMLINCLUDES = ../xapi ../database
OCAMLPACKS    =  xml-light2 uuid sexpr xcp stunnel http-svr gzip uuid xcp-inventory

OCamlProgram(xapi-db-upgrade-4.2, xapi-db-upgrade-4.2)
OCamlProgram(xapi-db-process, xapi_db_process)

.PHONY: install
install:
	mkdir -p $(DESTDIR)$(OPTDIR)/bin
	cp -f xapi-db-process $(DESTDIR)$(OPTDIR)/bin

.PHONY: clean
clean:
	rm -rf $(CLEAN_OBJS) *.aux *.log *.fig xapi xapi-db-process
